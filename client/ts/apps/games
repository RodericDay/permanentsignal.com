function setupChess() {
    [...`
    ♜♞♝♛♚♝♞♜
    ♟♟♟♟♟♟♟♟
    ・・・・・・・・
    ・・・・・・・・
    ・・・・・・・・
    ・・・・・・・・
    ♙♙♙♙♙♙♙♙
    ♖♘♗♕♔♗♘♖
    `].filter(c=>c!=='\n').forEach(function(s, i){
        if(s !== "・") {
            magnets.push([["div.magnet.piece[text=${s}]"], 40*(i%8), 40*(i/8|0), i]);
        }
    });
}
